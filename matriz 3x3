# criando uma matriz 3x3
matriz = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9],
]

# acessando um elemento
print(matriz[1] [2]) # saída 6

#alterando um elemento
matriz[0] [0] = 10
print(matriz)  # saída: [[10, 2, 3]], [4, 5, 6], [7, 8, 9,]]

#iterando sobre a matriz
for linha in matriz:
    for elemento in linha:
         print(elemento, end=' ')
    print() # para nova linha
